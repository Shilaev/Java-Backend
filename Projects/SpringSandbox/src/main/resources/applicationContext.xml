<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="musicBean" class="shilaev.MusicPlayerExample.Deathcore"/>

    <bean id="musicPlayerBean" class="shilaev.MusicPlayerExample.MusicPlayer">
        <constructor-arg ref="musicBean"/>
    </bean>

    <bean id="carBean" class="shilaev.CarExample.Car">
        <constructor-arg value="Mozeratti"/>
    </bean>

    <bean id="mobilePhoneBean" class="shilaev.MobileExample.MobilePhone">
        <constructor-arg value="Samsung"/>
    </bean>
</beans>